{
  "name": "ga",
  "version": "1.0.0",
  "description": "A toy programming language",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prettier:check": "prettier --check \"./**/*.{js,ts,json}\"",
    "prettier:fix": "prettier --write \"./**/*.{js,ts,json}\"",
    "clean": "rm -rf dist",
    "build": "pnpm run clean && tsc",
    "test": "pnpm run build && node --test ./dist/tests/**/*.js",
    "exec": "pnpm run build && node dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/x-incubator/ga.git"
  },
  "keywords": [
    "devanagari",
    "nepali",
    "nepali-programming-language",
    "programming-language"
  ],
  "author": "Basanta Rai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/x-incubator/ga/issues"
  },
  "homepage": "https://github.com/x-incubator/ga#readme",
  "engines": {
    "node": ">=20.x.x",
    "npm": ">=10.x.x"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}
